FoamFile
{
    version     2.0;
    format      ascii;
    class       dictionary;
    location    "constant";
    object      fvOptions;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include "../meshSetup"

turbine1
{
  type     $TURBINE;
  active   on;

  $TURBINE_COEFFS
    {
      fields  (U);

      selectionMode  cellSet;
      cellSet        turbine1;
      diskDir        (1 0 0);
      dragDir        (0 1 0);
      Cp             $CP1;
      Ct             $CT1;
      diskArea       $DISK_AREA;
      sigma          $DELTA;
      coeffs         (0.1 0.5 0.1); // radial distribution coefficients
      upstreamPoint  ($r1c1x $r1c1y $r1c1z);
    }
}

